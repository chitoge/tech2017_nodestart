<!DOCTYPE html>
<html>
  <head>
    <title>Userlist</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>User List</h1>
    <ul>
    	<% userlist.forEach(function(user) { %>
        <li><a href='<%= "mailto:" + user.email %>'><%= user.username %></a></li>
    	<% }); %>
    </ul>
    <a href="/newuser">Create a new user</a>
  </body>
</html>
